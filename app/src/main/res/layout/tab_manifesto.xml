    <!-- the number of people-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:layout_marginBottom="1dp"
        android:background="#FFF"
        android:gravity="center"
        android:layout_alignParentTop="true"
        android:id="@+id/manifestoParty">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                style="@style/manifestoPartyImage"
                android:src="@drawable/party_minju"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/manifestoPartyText"
                android:text="민주당 21명"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:background="@android:color/holo_red_dark"
                style="@style/manifestoPartyImage"
                android:src="@drawable/party_free_white"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/manifestoPartyText"
                android:text="한국당 5명"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:layout_width="24dp"
                android:layout_height="24dp"
                style="@style/manifestoPartyImage"
                android:background="@android:color/darker_gray"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/manifestoPartyText"
                android:text="무소속 1명"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorBackgroundGray"
        android:layout_alignBottom="@id/manifestoParty"
        android:id="@+id/manifestoPartyB"
        android:orientation="horizontal" />
